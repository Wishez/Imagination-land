{
	"manifest_version": 1,
	"name": "Content Filter",
	"description": "The plugin hides unwished content you don't want see in Internet.",
	"version": "0.1",
	"default_locale": "en",
	"manifest_version": 2,
	"icons": {
		"16": "calculator-16.png",
		"128": "calculator-128.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"main.js"
			]
		}
	],
	"permissions": [
        "tabs",
        "<all_urls>"
    ],
     "browser_action": {
        "default_popup": "window.html"
    }
}