{
	"manifest_version": 1,
	"name": "Content Filter",
	"description": "The plugin hides unwished content you don't want see in Internet.",
	"version": "0.1",
	"default_locale": "en",
	"manifest_version": 2,
	"icons": {
		"16": "calculator-16.png",
		"128": "calculator-128.png"
	},
	"app": {
		"background": {
			"scripts": ["background.js"]
		},
		"urls": [
			"http://certification.googleapps.com/app-info/"
		],
		"content_scripts": [
    	{
      		"matches": ["http://www.google.com/*"],
      		"css": ["main.css"],
      		"js": ["main.js"]
    	}
	  	],
		"launch": {
			"web_url": "http://certification.googleapps.com/"
		}
		},
		"permissions": [
			"unlimitedStorage",
			"notifications",
			"http://*/"
		],
		"browser_action": {
			"default_title": "On",
			"default_icon": {
				"16": "calculator-16.png",
				"128": "calculator-128.png"
			},
			"default_popup": "window.html"
		}
}